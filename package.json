{
  "name": "techmind-web",
  "version": "1.0.0",
  "description": "Techmind Partners website with clean URL routing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:optimized": "node server-optimized.js",
    "dev": "nodemon server.js",
    "dev:optimized": "nodemon server-optimized.js",
    "build": "node css/build/optimize.js",
    "build:critical": "node css/build/critical-extractor.js",
    "build:performance": "node css/build/performance-monitor.js",
    "build:all": "npm run build:critical && npm run build && npm run build:performance",
    "serve": "http-server . -p 8080 -c-1 --cors"
  },
  "dependencies": {
    "express": "^4.18.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "http-server": "^14.1.1"
  },
  "keywords": [
    "website",
    "techmind",
    "routing",
    "clean-urls"
  ],
  "author": "Techmind Partners",
  "license": "MIT"
}
